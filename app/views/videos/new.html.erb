<div class = "well">
   <%= form_for @video, html: { multipart: true } do |f| %>
      <%= f.label :Title %>
      <%= f.text_field :title %>
      <%= f.label :File %>
      <%= f.file_field :attachment %>
      <%= f.submit "Save", class: "btn btn-primary" %>
   <% end %>
</div>

<%=link_to "Back to Videos", videos_path %>
